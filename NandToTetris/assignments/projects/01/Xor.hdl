/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = nota);
    Not(in = b, out = notb);
    
    And(a = a, b = notb, out = term1);
    And(a = b, b = nota, out = term2);
    Or(a = term1, b = term2, out = out); 
}
